<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="model">
	<class name="Group" table="groupe">
		<id name="group_ID" type="long" column="group_ID">
			<generator class="native" />
		</id>
		<version name="version" access="field" column="version"/>

		<property name="groupName">
			<column name="groupName"></column>
		</property>

		<!-- Un groupe possÃ¨de 0, 1 ou n contact(s) -->
		<set name="contacts" table="contact_group" inverse="true" cascade="all" lazy="true">
			<key column="group_ID"></key>
			<many-to-many column="contact_ID" class="Contact"></many-to-many>
		</set>

	</class>
</hibernate-mapping>